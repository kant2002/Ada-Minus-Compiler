# 19 "1A15.c" 2


 



bool	is_private_constant (t, x)
ac	t,
	x;
{
	ac	type1,
		type2;

	 ;

	if (((* t )._acctype.d)  != 22 )
	   return 0 ;

	type1 = ((* ((* t )._object.desc)  )._objdesc.objtype) ;
	type2 = ((* ((* x )._object.desc)  )._objdesc.objtype) ;

	if (type1 != type2)
	   return 0 ;

	if (((* type1 )._acctype.d)  != 18 )
	   return 0 ;

	if (((* ((* t )._object.desc)  )._objdesc.expr)  == 0  &&
	    ((* ((* x )._object.desc)  )._objdesc.expr)  != 0 )
	   return 1 ;

	return 0 ;
}

bool	is_local_private (t)
ac	t;
{
	if (t == 0  || ((* t )._acctype.d)  != 18 )
	   return 0 ;

	if (env == 0  || ((* t )._acctype.enclunit)  != env)
	   return 0 ;

	return 1 ;
}

bool	is_sizeable (x)
ac	x;
{
	if (((* x )._acctype.d)  == 22 )
	   return 1 ;

	if (!is_type (x))
	   return 0 ;

	x = rootfilter (x);

	switch (((* x )._acctype.d) ) {
	   case 25 :
		return 0 ;

	   case 15 :
	        if (((* x )._variant.ffield)  == 0 )
	           return 1 ;		 
	        if (!((((* ((*  ((* x )._variant.ffield)   )._object.desc)  )._acctype.flags)  & 07) == 4 ) )
	           return 1 ;		 
	        if (((* ((* ((* x )._variant.ffield)  )._object.desc)  )._objdesc.expr)  != 0 )
	           return 1 ;		 
	        return 0 ;

	   default:
		return 1 ;
	}
}

bool	is_accesstype (x)
ac	x;
{
	if (x == 0 )
	   return 0 ;

	x = base_type (x);
	return ((* x )._acctype.d)  == 26 ;
}

 



bool	has_eq_and_ass (x)
ac	x;
{
	while (1 ) {
	   if (x == 0 )
	      return 0 ;

	   switch (((* x )._acctype.d) ) {
	      case 14 :
	         if ((((* x )._acctype.flags)  & 	0x7000 ) != 	0x7000 )
	            return 1 ;
	         0 ;

	      case 18 :
		 if ((((* x )._acctype.flags)  & 	0x7000 ) != 	0x7000 )
	            return 1 ;
		 if (in_spec (x))
		    return 1 ;
		 return 0 ;

	      case 24 :
	      case 33 :
	      case 26 :
	         return 1 ;

	      case 17 :
	         x = ((* x )._incomplete.complete) ;
	         break;

	      case 27 :
	         x = ((* x )._subtype.parenttype) ;
	         break;

	      case 38 :
	         x = ((* x )._filter.filtype) ;
	         break;

	      case 25 :
	         return ((* x )._acctype.flags)  & 02 ;

	      case 15 :
	         return ((* x )._acctype.flags)  & 02 ;

	      case 29 :
	         x = ((* x )._newtype.old_type) ;
	         break;

	      case 12 :
		 return 0 ;

	      default:
		if (x != type_of (x)) {
	           x = type_of (x);
		   break;
		}
		 ;
	   }
	}
}

 


bool	is_assignable (x)
ac	x;
{
	return is_object (x) && has_eq_and_ass (type_of (x));
}

 


bool	has_slicing (t)
ac	t;
{
	return get_slicetypes (t) != 0 ;
}

 


bool	has_selection (t)
ac	t;
{
	if (t == 0 )
	   return 0 ;

	t = base_type (t);

	switch (((* t )._acctype.d) ) {
	   case 15 :
	   case 12 :
		return 1 ;

	   case 7 :
	   case 3 :
	   case 5 :
	   case 4 :
	   case 0 :
		return is_openscope (t);

	   case 2 :
		return !((((*  t  )._acctype.flags)  & 0200) == 0200)  || is_openscope (t);

	   default:
		return get_selecttype (t) != 0 ;
	}
}

 


bool	has_indexing (t)
ac	t;
{
	if (t == 0 )
	   return 0 ;

	return get_arraytype (t) != 0 ;
}

 


bool	has_calling (t)
ac	t;
{
	while (1 ) {
	   if (t == 0 )
	      return 0 ;

	   switch (((* t )._acctype.d) ) {
	      case 85 :
	      case 1 :
	      case 0 :
	      case 21 :
	      case 28 :
		 return 1 ;

	      case 27 :
	         t = ((* t )._subtype.parenttype) ;
	         break;

	      case 38 :
	         t = ((* t )._filter.filtype) ;
	         break;

	      case 26 :
	         t = ((* t )._acctype.actype) ;
	         break;

	      default:
		 return 0 ;
	   }
	}
}

 


bool	has_indexconstraint (t)
ac	t;
{
	while (1 ) {
	   if (t == 0 )
	      return 0 ;

	   switch (((* t )._acctype.d) ) {
	      case 25 :
	         return 1 ;

	      case 29 :
	         t = ((* t )._newtype.old_type) ;
	         break;

	      case 26 :
	         t = ((* t )._acctype.actype) ;
	         break;

	      case 27 :
	         t = ((* t )._subtype.parenttype) ;
		 if (t != 0  && ((* t )._acctype.d)  == 38 )
		    return 0 ;
	         break;

	      default:
		return 0 ;
	   }
	}
}

 


bool	has_discriminantconstraint (t)
ac	t;
{
	while (1 ) {
	   if (t == 0 )
	      return 0 ;

	   switch (((* t )._acctype.d) ) {
	      case 29 :
	         t = ((* t )._newtype.old_type) ;
	         break;

	      case 15 :
	         return ((((* ((*  ((* t )._variant.ffield)   )._object.desc)  )._acctype.flags)  & 07) == 4 ) ;

	      case 18 :
		 return ((* t )._privtype.fpdiscr)  != 0 ;
 








	      case 17 :
	         return ((* t )._incomplete.fidiscr)  != 0 ;

	      case 26 :
	         t = ((* t )._acctype.actype) ;
	         break;

	      default:
		 return 0 ;

	   }
	}
}

 


bool	is_scalar (t)
ac	t;
{
	while (1 ) {
	   if (t == 0 )
	      return 0 ;

	   if (is_someinteger (t))
	      return 1 ;

	   switch (((* t )._acctype.d) ) {
	      case 17 :
	         t = ((* t )._incomplete.complete) ;
	         break;

	      case 18 :
	          if (!in_spec (t))
	             return 0 ;

	          t = ((* t )._privtype.impl) ;
	          break;

	      case 27 :
	         t = ((* t )._subtype.parenttype) ;
	         break;

	      case 38 :
	         t = ((* t )._filter.filtype) ;
	         break;

	      case 29 :
	         t = ((* t )._newtype.old_type) ;
	         break;

	      case 24 :
	         return 1 ;

	      case 14 :
		 return ((* t )._genpartype.genkind)  == 6  ||
		       ((* t )._genpartype.genkind)  == 1 ;

	   default:
		return 0 ;
	   }
	}
}

 


bool is_float (t)
ac	t;
{
	while (1 ) {
	   if (t == 0 )
	      return 0 ;

	   if (t == std_real)
	      return 1 ;

	   switch (((* t )._acctype.d) ) {
	      case 27 :
	         t = ((* t )._subtype.parenttype) ;
	         break;

	        case 38 :
	         t = ((* t )._filter.filtype) ;
	         break;

	      case 17 :
	         t = ((* t )._incomplete.complete) ;
	         break;

	      case 18 :
	         if (!in_spec (t))
	            return 0 ;
	         t = ((* t )._privtype.impl) ;
	         break;

	      case 29 :
	         t = ((* t )._newtype.old_type) ;
	         break;

	      case 14 :
		 return ((* t )._genpartype.genkind)  == 2 ;

	      default:
		 return 0 ;
	   }
	}
}

 


bool	is_numerical (t)
ac	t;
{
	return is_someinteger (t) || is_float (t);
}

 



bool	is_basetype (x)
ac	x;
{
	if (x == 0 )
	   return 0 ;

	switch (((* x )._acctype.d) ) {
	   case 14 :
	   case 26 :
	   case 24 :
	   case 17 :
	   case 18 :
	   case 25 :
	   case 15 :
	   case 33 :
	   case 29 :
	   case 12 :
	      return 1 ;

	   default:
	      return 0 ;
	}
}

 


bool	has_type (t)
ac	t;
{
	switch (((* t )._acctype.d) ) {
	   case 10 :
	   case 11 :
	   case 22 :
	   case 19 :
	   case 28 :
 

	   case 6 :
	   case 0 :
	   case 21 :
	      return 1 ;

	   default:
	      return 0 ;
	}
}

 


bool	is_type (t)
ac	t;
{
	if (t == 0 )
	   return 0 ;

	switch (((* t )._acctype.d) ) {
	   case 27 :
	   case 38 :
	   case 14 :
	   case 26 :
	   case 24 :
	   case 17 :
	   case 18 :
	   case 25 :
	   case 15 :
	   case 33 :
	   case 29 :
	   case 12 :
	      return 1 ;

	   default:
	      return 0 ;
	}
}

 



bool	is_object (obj)
ac	obj;
{
	int	t;

	while (1 ) {
	   if (obj == 0 )
	      return 0 ;

	   switch (((* obj )._acctype.d) ) {
	      case 22 :
	         t = (((*  ((* obj )._object.desc)   )._acctype.flags)  & 07) ;
	         return !(t == 2  || t == 1  || t == 5 );

	      case 47 :
	         obj = ((* obj )._name.fentity) ;
	         break;

	      case 49 :
	      case 50 :
	      case 51 :
	         return 0 ;

	      case 53 :
	         return 1 ;

	      case 54 :
	      case 52 :
	      case 48 :
	         obj = ((* obj )._acctype.next) ;
	         break;

	      case 19 :
	         obj = ((* obj )._renobj.name) ;
	         break;

	      default:
	         return 0 ;
	   }
	}
}

 


bool	is_enumlit (f)
ac	f;
{
	if (((* f )._acctype.d)  == 10 )
	   return 1 ;

	return 0 ;
}

bool	is_charlit (f)
ac	f;
{
	return ((* f )._acctype.d)  == 11 ;
}

 


bool	is_sub (f)
ac	f;
{
	switch (((* f )._acctype.d) ) {
	   case 0 :
	   case 21 :
	   case 1 :
	   case 85 :
	   case 28 :
	      return 1 ;

	   default:
	      return 0 ;
	}
}

 


bool is_sttype (x)
ac	x;
{
	if (((((*  x  )._acctype.flags)  & 060) == 040) )
	   return 1 ;

	if (((* x )._acctype.d)  == 29 )
	   return is_sttype (((* x )._newtype.old_type) );

	return 0 ;
}

 



bool	is_paramless (x)
ac	x;
{
	ac	y;

	if (x == 0  || !is_sub (x))
	   return 0 ;

	for ( y  = (  f_formal (x) );  y  != 0 ;  y  = ((*  y  )._acctype.next) )  {
	   if (!(((* ((*  y  )._object.desc)  )._objdesc.expr)  != (ac) 0 ) )
	      return 0 ;
	}

	return 1 ;
}

 


bool	is_entype (f)
ac	f;
{
	if (f == 0 )
	   return 0 ;

	while (((* f )._acctype.d)  == 29 )
	   f = ((* f )._newtype.old_type) ;

	return f != 0  && ((* f )._acctype.d)  == 24 ;
}

 



bool	is_strtype (f)
ac	f;
{
	if (f == 0 )
	   return 0 ;

	f = base_type (f);
	while (((* f )._acctype.d)  == 29 )
	   f = base_type (((* f )._newtype.old_type) );

	return f != 0  && ((* f )._acctype.d)  == 25 ;
}


 


bool	is_someinteger (x)
ac	x;
{
	if (x == 0 )
	   return 0 ;

	x = base_type (x);
	if (x == std_integer || x == std_short || x == std_long)
	   return 1 ;

	if (x == std_univ)
	   return 1 ;

	if (((* x )._acctype.d)  == 29 )
	   return ((* x )._newtype.mark)  == 1 ;

	if (((* x )._acctype.d)  == 14 )
	   return ((* x )._genpartype.genkind)  == 1 ;

	return 0 ;
}

 


bool	is_bool_vect (x)
ac	x;
{
	ac	t;

	t = get_arraytype (x);
	if (t == 0 )
	   return 0 ;

	if (((* ((* t )._arraytype.findex)  )._acctype.next)  != 0 )
	   return 0 ;
 



	return base_type (((* t )._arraytype.elemtype) ) == std_boolean;
}

 


bool	is_sc_vector (x)
ac	x;
{
	if (x == 0 )
	   return 0 ;

	x = base_type (x);
	 

	if (((* x )._acctype.d)  == 33  && ((* x )._internal.ind)  == 9 )
	   return 1 ;

	x = get_arraytype (x);
	if (x == 0 )
	   return 0 ;

	if (((* ((* x )._arraytype.findex)  )._acctype.next)  != 0 )
	   return 0 ;

	return is_scalar (((* ((* x )._arraytype.findex)  )._index.indextype) );
}

 


bool	is_anytype (x)
ac	x;
{
	x = base_type (x);
	if (x == 0  || ((* x )._acctype.d)  != 33 )
	   return 0 ;

	switch (((* x )._internal.ind) ) {
	   case 	12 :
	   case 1 :
	   case 4 :
	   case 9 :
	   case 	6 :
	   case 2 :
	   case 3 :
	   case 5 :
	      return 1 ;

	   default:
	      return 0 ;
	}

}

 


bool	is_call (x)
ac	x;
{
	if (x == 0 )
	   return 0 ;

	if (((* x )._acctype.d)  != 47 )
	   return 0 ;

	if (!is_sub (((* x )._name.fentity) ))
	   return 0 ;	

	return 1 ;
}

 


bool (((*  r  )._range.rangetype)  == 0  && ((*  r  )._range.frang_exp)  == 0 ) 
ac	r;
{
	 ;
	 ;

	return ((* r )._range.rangetype)  == 0  && ((* r )._range.frang_exp)  == 0 ;
}

 


bool	is_derived (x)
ac	x;
{
	return x != 0  && ((* x )._acctype.d)  == 28 ;
}

 


bool	is_stringlit (x)
ac	x;
{
	if (x == 0 )
	   return 0 ;

	if (((* x )._acctype.d)  != 60 )
	   return 0 ;

	if (((* x )._literal.val)  == 0 )
	   return 0 ;

	if (((* x )._literal.val)  -> litkind == 3 )
	   return 1 ;

	return 0 ;
}

 


bool	is_stringtype (x)
ac	x;
{
	ac	y,
		z;

	x = get_arraytype (x);
	if (x == 0 )
	   return 0 ;

	y = ((* x )._arraytype.findex) ;
	if (y == 0 )
	   return 0 ;

	if (((* y )._acctype.next)  != 0 )
	   return 0 ;

	if (((* y )._index.indextype)  == any_type || is_someinteger (((* y )._index.indextype) )) {
 


	   z = ((* x )._arraytype.elemtype) ;
	   if (!is_entype (z))
	      return 0 ;

	   for ( y  = (  fenum_of (z) );  y  != 0 ;  y  = ((*  y  )._acctype.next) )  {
	      if (is_charlit (y))
	         return 1 ;
	   }

	}

	return 0 ;
}


 



bool	is_object_or_value (x)
ac	x;
{
	if (x == 0 )
	   return 0 ;

	if (((* x )._acctype.d)  != 47 )
	   return 1 ;

	if (is_type (((* x )._name.fentity) ))
	   return 0 ;

	return 1 ;
}

 


bool	is_tpname (x)
ac	x;
{
	ac	y;

	 ;
	 ;

	y = ((* x )._exp.primary) ;
	if (y == 0 )
	   return 0 ;

	if (((* y )._acctype.d)  != 47 )
	   return 0 ;

	if (((* y )._name.fentity)  == 0 )
	   return 0 ;

	if (is_type (((* y )._name.fentity) ))
	   return 1 ;

	return 0 ;
}

 



bool	is_name (x)
ac	x;
{
	if (x == 0 )
	   return 0 ;

	switch (((* x )._acctype.d) ) {
	   case 53 :
	   case 49 :
	   case 50 :
	   case 51 :
	   case 54 :
	   case 48 :
	   case 52 :
	   case 47 :
		return 1 ;

	   default:
		return 0 ;
	}
}

 


bool	is_ecall (x)
ac	x;
{
	if (x == 0 )
	   return 0 ;

	if (((* x )._acctype.d)  != 48 )
	   return 0 ;

	if (((* ((* x )._all.types)  )._acctype.d)  == 1 )
	   return 1 ;

	if (((* ((* x )._all.types)  )._acctype.d)  == 85 )
	   return ((* ((* ((* x )._all.types)  )._triple.tripsub)  )._acctype.d)  == 1 ;

	return 0 ;
}

 


bool	is_positional (x)
ac	x;
{
	if (x == 0 )
	   return 0 ;

	if (((* x )._acctype.d)  != 46 )
	   return 1 ;

	return ((* x )._namedass.parname)  == 0 ;
}

 



bool	is_loop (x)
ac	x;
{
	switch (((* x )._acctype.d) ) {
	   case 9 :
	   case 8 :
	      return 1 ;

	   case 32 :
	      return is_loop (((* x )._tagstat.t_stat) );

	   default:
	      return 0 ;
	}
}


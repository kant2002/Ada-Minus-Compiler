separate (auto_control)
procedure put_all (dyn  : in dyn_type) is
begin
    put_helling (dyn .helling);
    put_gas_rem (dyn .gas_stand);
    put_max_gas (dyn .max_gas);
    put_max_rem (dyn .max_rem);
    put_crash   (dyn .crash);
    put_new_v   (dyn .new_v);
    put_tijdstip(dyn .tijdstip);
end put_all;

